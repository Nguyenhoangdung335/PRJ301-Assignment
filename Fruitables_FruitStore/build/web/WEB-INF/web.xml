        <servlet-name>GetProductTypeServlet</servlet-name>
        <servlet-class>ServletManager.FunctionalServlet.GetProductTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductUpdateServlet</servlet-name>
        <servlet-class>ServletManager.FunctionalServlet.ProductUpdateServlet</servlet-class>
