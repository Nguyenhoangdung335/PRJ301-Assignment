    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>ServletManager.PageGetServlet.CartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>ServletManager.FunctionalServlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartDeleteServlet</servlet-name>
        <servlet-class>ServletManager.FunctionalServlet.CartDeleteServlet</servlet-class>
    </servlet>
