        <servlet-class>ServletManager.DisplayItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DispatchServlet</servlet-name>
        <servlet-class>ServletManager.DispatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>ServletManager.PageGetServlet.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAddServlet</servlet-name>
